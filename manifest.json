{
    "name": "Q-SKU",
    "description": "Allow adding additional Listing SKU details for for each Master SKU through submitted purchase orders",
    "version": "0.0.1",
    "icons": {
        "16": "icon-20.png",
        "128": "icon-20.png"
    },
    "manifest_version": 2,
    "permissions": [
        "notifications"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "content_scripts" : [
        {
            "css" : ["main.css"],
            "matches": ["https://app.skubana.com/work/po*"],
            "js": [
                "libs/jquery-3.3.1.slim.min.js",
                "libs/MutationSummary.js",
                "injected.js"
            ]
        }
    ]
}
